{
  "name": "admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\" \"*.{ts,js}\" \"*.config.{ts,js}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css}\" \"*.{ts,js}\" \"*.config.{ts,js}\"",
    "docs:dev": "npm --prefix docs run dev",
    "docs:build": "npm --prefix docs run build",
    "docs:gen:typedoc": "typedoc --options typedoc.json --json tmp/typedoc.json",
    "docs:gen:jsdoc": "documentation build \"src/**\" -f json -o tmp/jsdoc.json",
    "docs:gen:react": "cross-env TS_NODE_PROJECT=tsconfig.docgen.json node --loader ts-node/esm scripts/react-docgen.ts",
    "docs:gen": "cross-env TS_NODE_PROJECT=tsconfig.docgen.json node --loader ts-node/esm scripts/docgen.ts",
    "docs:check": "cross-env TS_NODE_PROJECT=tsconfig.docgen.json node --loader ts-node/esm scripts/docgen.ts --check && markdownlint \"docs/src/content/**/*.md\" && remark \"docs/src/content\" --frail && node scripts/run-markdown-link-check.mjs",
    "lint-staged": "lint-staged"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "documentation": "^14.0.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "lint-staged": "^15.4.3",
    "markdown-link-check": "^3.14.1",
    "markdownlint-cli": "^0.44.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "react-docgen-typescript": "^2.4.0",
    "remark": "^15.0.1",
    "remark-cli": "^12.0.1",
    "remark-preset-lint-recommended": "^7.0.1",
    "tailwindcss": "^4.1.17",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.14",
    "typedoc-plugin-markdown": "^4.3.4",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.1.7",
    "vitest": "^2.1.5"
  },
  "dependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "mobx": "^6.15.0",
    "mobx-react-lite": "^4.1.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.5"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,md,mdx}": [
      "npm run docs:gen",
      "npm run docs:check"
    ]
  }
}
